<table border="1">
    <tr>
        <td>Id</td>
        <td>Username</td>
        <td>Email</td>
    </tr>
    {% for a in tabAuthors %}
    <tr>
        <td>{{ a.id }}</td>
        <td>{{ a.username }}</td>
        <td>{{ a.email }}</td>
        <td><a href="{{ path('edit', {'id': a.id}) }}">edit</a></td>
        <td><a href="{{ path('delete', {'id': a.id}) }}">delete</a></td>
    </tr>
    {% endfor %}
</table>
